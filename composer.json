{
    "name": "dgtlss/capsule",
    "description": "A Laravel package for backing up databases and files to external sources with notifications",
    "type": "library",
    "license": "MIT",
    "version": "1.1.0",
    "authors": [{
        "name": "Nathan Langer",
        "email": "nathanlanger@googlemail.com"
    }],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/mail": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "league/flysystem": "^3.0"
    },
    "suggest": {
        "league/flysystem-aws-s3-v3": "Required for S3/DigitalOcean Spaces storage (^3.0)",
        "league/flysystem-ftp": "Required for FTP storage (^3.0)",
        "league/flysystem-sftp-v3": "Required for SFTP storage (^3.0)"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dgtlss\\Capsule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dgtlss\\Capsule\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dgtlss\\Capsule\\CapsuleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}